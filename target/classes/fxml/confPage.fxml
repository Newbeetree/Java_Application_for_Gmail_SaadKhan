        <PasswordField fx:id="DBPassIn" layoutX="220.0" layoutY="518.0"/>
        <Button fx:id="submitBtn" layoutX="175.0" layoutY="580.0" mnemonicParsing="false"
                onAction="#submitCredentials" text="%submitBtn"/>
        <Label fx:id="jagTxt" layoutX="57.0" layoutY="47.0" text="%jagTxt" textAlignment="CENTER">
            <font>
                <Font size="21.0"/>
            </font>
        </Label>
        <MenuBar prefHeight="31.0" prefWidth="497.0">
            <menus>
                <Menu fx:id="fileMn" mnemonicParsing="false" text="%fileMn">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu fx:id="editMn" mnemonicParsing="false" text="%editMn">
                    <items>
                        <MenuItem mnemonicParsing="false" text="%delete"/>
                    </items>
                </Menu>
                <Menu fx:id="helpMn" mnemonicParsing="false" text="%helpMn">
                    <items>
                        <MenuItem mnemonicParsing="false" text="%about"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
